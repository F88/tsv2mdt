{
  "name": "tsv2mdt",
  "description": "Webapp for converting TSV files to Markdown tables.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "keywords": [
    "typescript",
    "tsv",
    "markdown",
    "converter",
    "webapp"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F88/tsv2mdt.git"
  },
  "bugs": {
    "url": "https://github.com/F88/tsv2mdt/issues"
  },
  "homepage": "https://github.com/F88/tsv2mdt-e2e#readme",
  "scripts": {
    "dev": "vite",
    "prettier:check": "prettier --check \"{src,test,scripts}/**/*.{js,json,mjs,ts,jsx,tsx}\" \"*.{md,json,ts}\"",
    "prettier:write": "prettier --write \"{src,test,scripts}/**/*.{js,json,mjs,ts,jsx,tsx}\" \"*.{md,json,ts}\"",
    "format": "npm run prettier:write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:codecov": "vitest run --coverage --reporter=default --reporter=junit --outputFile.junit=test-report.junit.xml",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.2.0",
    "@mui/material": "7.2.0",
    "dompurify": "3.2.6",
    "firebase": "12.0.0",
    "i18next": "25.3.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-i18next": "15.6.1",
    "x2md": "1.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.0.1",
    "@eslint/js": "9.32.0",
    "@storybook/addon-a11y": "9.0.18",
    "@storybook/addon-docs": "9.0.18",
    "@storybook/addon-vitest": "9.0.18",
    "@storybook/react-vite": "9.0.18",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.1.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react-swc": "3.11.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.3.4",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-dom": "1.52.3",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-react-x": "1.52.3",
    "eslint-plugin-storybook": "9.0.18",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "prettier": "3.6.2",
    "storybook": "9.0.18",
    "typescript": "5.8.3",
    "typescript-eslint": "8.38.0",
    "vite": "7.0.6",
    "vitest": "3.2.4"
  }
}
